{
  "compileOnSave": false,
  "compilerOptions": {
    "baseUrl": "./",
    "outDir": "./dist/out-tsc",
    "sourceMap": true,
    "declaration": false,
    "module": "esnext",
    "moduleResolution": "node",
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "target": "es2015",
    "resolveJsonModule": true,
    "typeRoots": [
      "node_modules/@types"
    ],
    "lib": [
      "esnext.array",
      "es2018",
      "dom"
    ],
    "paths": {
      "captcha": [
        "dist/captcha"
      ],
      "captcha/*": [
        "dist/captcha/*"
      ],
      "modal": [
        "dist/modal"
      ],
      "modal/*": [
        "dist/modal/*"
      ],
      "core": [
        "dist/core"
      ],
      "core/*": [
        "dist/core/*"
      ],
      "loading": [
        "dist/loading"
      ],
      "loading/*": [
        "dist/loading/*"
      ],
      "@logo-software/language": [
        "dist/logo-software/language"
      ],
      "@logo-software/language/*": [
        "dist/logo-software/language/*"
      ],
      "@logo-software/excel": [
        "dist/logo-software/excel"
      ],
      "@logo-software/excel/*": [
        "dist/logo-software/excel/*"
      ],
      "@logo-software/paging": [
        "dist/logo-software/paging"
      ],
      "@logo-software/paging/*": [
        "dist/logo-software/paging/*"
      ],
      "@logo-software/table": [
        "dist/logo-software/table"
      ],
      "@logo-software/table/*": [
        "dist/logo-software/table/*"
      ]
    }
  }
}
